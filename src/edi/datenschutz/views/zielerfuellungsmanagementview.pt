       <!--<div class="col-12 col-md-8 mb-3">-->
        <div class="col-12 mb-3">
        <table class="table table-bordered">
          <thead>
            <tr>
              <th scope="col">ID</th>
              <th scope="col">Schwachstelle</th>
              <th scope="col">Risikoquelle</th>
              <th scope="col">Risiko-Szenario</th>
            </tr>
          </thead>
          <tbody>
            <tr tal:repeat="object view/verfuegbarkeit">
              <td tal:content="object/id"></td>
              <td tal:content="object/schwachstelle"></td>
              <td tal:content="object/quelle"></td>
