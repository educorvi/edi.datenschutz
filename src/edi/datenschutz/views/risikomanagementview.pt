<html xmlns="http://www.w3.org/1999/xhtml"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      i18n:domain="edi.datenschutz"
      metal:use-macro="context/main_template/macros/master">

    <body>

   <metal:main fill-slot="main">
     <tal:main-macro metal:define-macro="main">

       <div class="container">

         <div tal:replace="structure provider:plone.abovecontenttitle" />

         <h1 class="documentFirstHeading" tal:content="context/title" />

         <div tal:replace="structure provider:plone.belowcontenttitle" />

         <p class="documentDescription" tal:content="context/description" />

         <div tal:replace="structure provider:plone.abovecontentbody" />

         <div tal:replace="structure provider:plone.belowcontentbody" />



      <div class="row mb-3">
        <div class="col-12 col-md-8">
            <h2>Verf√ºgbarkeit</h2>
        </div>
        <div class="col-12 col-md-1">
        </div>
        <div class="col-12 col-md-3">
        </div>
      </div>

      <div class="row mb-3">
       <div class="col-12 col-md-8 mb-3">
        <table class="table table-bordered">
          <thead>
            <tr>
              <th scope="col">ID</th>
              <th scope="col">Schwachstelle</th>
              <th scope="col">Risikoquelle</th>
              <th scope="col">Risiko-Szenario</th>
            </tr>
          </thead>
          <tbody>
            <tr tal:repeat="kategorie context/verfuegbarkeit">
              <td tal:content="kategorie/id"></td>
              <td tal:content="kategorie/schwachstelle"></td>
              <td tal:content="kategorie/risikoquelle"></td>
              <td tal:content="kategorie/risikoszenario"></td>
            </tr>
          </tbody>
        </table>
       </div>
      </div>









    </div>

    </tal:main-macro>

   </metal:main>

 </body>

</html>
